{
    "compilerOptions": {
      "target": "es2016",
  
      "module": "ES2022",
      "esModuleInterop": true,         
      "forceConsistentCasingInFileNames": true,
  
      "allowJs": true,
      "checkJs": true,
  
      "outDir": "dist",
  
      "declaration": true,
      "emitDeclarationOnly": true,
  
      "strict": true,
  
      "skipLibCheck": false,
      "skipDefaultLibCheck": true,                               
    },
  }
  